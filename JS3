<script>
let x = 123;
document.getElementById("demo1").innerHTML =
  x.toString() + "<br>" +
   (321).toString() + "<br>" +
   (10 + 23).toString();
document.getElementById("demo2").innerHTML =x.toExponential()
  document.getElementById("demo3").innerHTML =x.toFixed(3)
let y=9.123;
document.getElementById("demo4").innerHTML =y.toPrecision(2)
document.getElementById("demo5").innerHTML =(12+10).valueOf()
document.getElementById("demo6").innerHTML = 
  Number(true) + "<br>" +
  Number(false)+ "<br>" +parseInt("-10")+ "<br>" +parseFloat("-10.99")
  + "<br>" +Number.isInteger(10)+ "<br>" +Number.isInteger(10.44)
</script>

